<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
	   http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="questionBank" 
	class="com.trainings.collections.QuestionBank">
		<property name="question" 
			value="Inheritence is available in ....."/>
	
		<property name="options">
			<list>
				<value>JAVA</value>
				<value>C++</value>
				<value>SCALA</value>
				<value>ALL</value>
			</list>
		</property>
	
	</bean>



	<bean id="addressSet" class="com.trainings.collections.AddressSet">
		<property name="addresses">
			<set>
				<value>Bengaluru, Mission Road</value>
				<value>Bengaluru, Mission Road</value>
				<value>Bengaluru, Richmond Road</value>
				<value>Bengaluru, Majestic</value>
				<value>Bengaluru, Koramangala</value>
			</set>
		</property>
	</bean>


	<bean id="accountMap" class="com.trainings.collections.AccountMap">
		<property name="accountMap">
			<map>
				<entry key="Ramu" value="4000"/>
				<entry key="Allahrakha" value="3456"/>
				<entry key="Pooja" value="4433"/>
				<entry key="Anuj Kumar" value="4435"/>
			</map>
		</property>
	</bean>

	<bean id="myCollection" class="com.trainings.collections.MyCollection"
		autowire="byName"/>









</beans>